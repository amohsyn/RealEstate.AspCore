@using Pages.Manage.User
@model RealEstate.Services.BaseLog.LogDetailViewModel

@if (Model != null)
{
    <text>
        <b>@SharedLocalizer[Model.Type.GetDisplayName()]:</b>
        @Model.DateTime.GregorianToPersian(false, true)
        @SharedLocalizer["By"]
        <a asp-page="@typeof(IndexModel).Page()" asp-route-userId="@Model.User.Id" target="_blank">@Model.User.FirstName @Model.User.LastName</a>
    </text>
}